{
  "name": "lzma-native",
  "version": "1.2.0",
  "engines": {
    "node": ">=0.10"
  },
  "author": {
    "name": "Anna Henningsen",
    "email": "sqrt@entless.org"
  },
  "description": "Provides bindings to the native liblzma library (.xz file format, among others)",
  "main": "index",
  "bin": {
    "lzmajs": "./bin/lzmajs"
  },
  "dependencies": {
    "any-promise": "^1.1.0",
    "commander": "^2.9.0",
    "nan": "^2.2.0",
    "node-pre-gyp": "^0.6.21",
    "readable-stream": "^2.0.5",
    "util-extend": "^1.0.3"
  },
  "bundledDependencies": ["node-pre-gyp"],
  "keywords": [
    "lzma",
    "compression",
    "crc32",
    "xz"
  ],
  "homepage": "https://github.com/addaleax/lzma-native",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/addaleax/lzma-native.git"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha --expose-gc -s 1000 -t 5000",
    "prepublish": "cd deps && 7z x -y xz-5.2.1-windows.7z bin_i686/liblzma.dll bin_x86-64/liblzma.dll include doc/liblzma.def",
    "jshint": "jshint ."
  },
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/addaleax/lzma-native/issues"
  },
  "devDependencies": {
    "bl": "^1.1.2",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "q": "^1.4.1"
  },
  "binary": {
    "module_name": "lzma_native",
    "module_path": "./binding/",
    "host": "https://node-pre-gyp.entless.org",
    "remote_path": "./lzma-native/"
  }
}
